{
  "expo": {
    "name": "Ribaru",
    "slug": "ribaru",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ribaru.app"
    },
    "android": {
      "package": "com.ribaru.app"
    },
    "scheme": "ribaru",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "newArchEnabled": true,
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
